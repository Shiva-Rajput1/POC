{
    "name": "HelloWorldApp",
    "version": "1.0.0",
    "private": true,
    "scripts": {
      "start": "expo start",
      "build": "expo build"
    },
    "dependencies": {
      "expo": "^49.0.0",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-native": "0.71.8",
      "react-native-web": "0.18.10"
    },
    "devDependencies": {
      "babel-preset-expo": "^9.0.0"
    },
    "main": "node_modules/expo/AppEntry.js",
    "jest": {
      "preset": "react-native"
    },
    "expo": {
      "name": "HelloWorldApp",
      "slug": "HelloWorldApp",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#FFFFFF"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      }
    }
  }
